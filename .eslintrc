{
 "extends": ["prettier", "react-app"],
 "plugins": ["prettier"],
 "settings": {
  "import/resolver": {
   "node": {
    "paths": ["src"]
   }
  }
 },
 "rules": {
  "prettier/prettier": "error",
  "no-unused-vars": "warn",
  "no-console": "off",
  "func-names": "off",
  "no-process-exit": "off",
  "object-shorthand": "off",
  "class-methods-use-this": "off",
  "react/jsx-filename-extension": [
   2,
   { "extensions": [".js", ".jsx", ".ts", ".tsx"] }
  ],
  "react/jsx-wrap-multilines": "off"
 }
}
